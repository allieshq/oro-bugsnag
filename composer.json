{
    "name": "allies/oro-bugsnag",
    "type": "symfony-bundle",
    "description": "BugSnag integration for OroCRM",
    "keywords":["CRM", "OroCRM", "Bugsnag"],
    "license":"MIT",
    "require": {
        "php": ">=5.6",
        "oro/platform": "2.*",
        "bugsnag/bugsnag-symfony": "1.*",
        "mead-steve/mono-snag": "^3.0.0"
    },
    "autoload": {
        "psr-0": { "Allies\\Bundle\\LogViewerBundle": "src/" }
    },
    "target-dir": "src/Allies/Bundle/OroBugsnagBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
